    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-sonarjs": "0.21.0",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "ts-loader": "9.5.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
