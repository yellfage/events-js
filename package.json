  "version": "1.0.0",
  "description": "Event management",
  "main": "dist/index.js",
  "author": "yellfage",
  "license": "MIT",
  "keywords": [
    "event",
    "events",
    "emit",
    "emiting",
    "emitter",
    "eventemitter",
    "event-emitter",
    "dispatcher",
    "eventdispatcher",
    "event-dispatcher",
    "listen",
    "pubsub",
    "observe",
    "observer",
    "intercept",
    "interceptor"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
